{
  "name": "gpapi",
  "description": "use google play protobuf api in node",
  "keywords": [
    "google",
    "play store",
    "play",
    "api",
    "crawler"
  ],
  "version": "4.1.0",
  "main": "index.js",
  "private": false,
  "homepage": "https://github.com/dweinstein/node-google-play",
  "bugs": {
    "url": "https://github.com/dweinstein/node-google-play/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dweinstein/node-google-play.git"
  },
  "files": [
    "scripts",
    "lib",
    "index.js"
  ],
  "semistandard": {
    "ignore": [
      "/lib/errors.js"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "test": "semistandard && tap test/*.test.js",
    "get-proto": "node scripts/get-latest-proto.js",
    "postinstall": "npm run get-proto"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "debug": "^2.2.0",
    "json-stable-stringify": "~1.0.0",
    "lodash": "^4.6.1",
    "protobufjs": "^5.0.1",
    "request": "^2.69.0",
    "request-debug": "^0.2.0",
    "split": "^1.0.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "minimist": "^1.1.1",
    "semistandard": "*",
    "tap": "*"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "author": "dweinstein",
  "license": "MIT"
}
